{
  "name": "A browser action with no icon that makes the page red",
  "manifest_version": 2,
  "version": "1.0",
  "background": {
	"scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": [
	"tabs",
	"bookmarks",
    "<all_urls>"
  ]
}
